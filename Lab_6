#include <iostream>
using namespace std;

int main()
{
    int N;
    cout << "Enter a natural number N: ";
    cin >> N;

    int sum = 0;

    while (N > 0)
    {
        int a = N % 10; 
        if (a > 5)
        {
            sum += a;
        }

        N /= 10;
    }

    cout << "Sum of digits greater than 5: " << sum << endl;

    return 0;
}
