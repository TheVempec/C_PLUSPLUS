#include <iostream>
using namespace std;

int main()
{
    int sum = 0;
    int N;

    cout << "Enter N-digit number: ";
    cin >> N;

    while (N > 0)
    {
        int a = N % 10; 
        cout << "a: " << a << endl;

        if (a > 5)
        {
            sum += a;
        }

        N /= 10; 
    }

    cout << "Result of addition: " << sum << endl;

    return 0;
}
